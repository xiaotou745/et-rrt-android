<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="@dimen/view_space"
    >

    <TextView
        android:id="@+id/gv_multipe_team_images_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="多组图片组"
        android:gravity="left"
        android:textColor="@color/tv_order_color_1"
        android:textSize="@dimen/_15sp"
        android:layout_marginBottom="@dimen/view_space"
        />
    <com.renrentui.view.MyGridView
        android:id="@+id/gv_multipe_team_images_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:numColumns="5"
        android:stretchMode="columnWidth"
        android:horizontalSpacing="@dimen/view_space_half"
        android:verticalSpacing="@dimen/view_space_half"
        ></com.renrentui.view.MyGridView>


</LinearLayout>
