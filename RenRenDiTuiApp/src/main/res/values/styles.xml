<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.







        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <!-- activity或者fragment页面顶部布局     相对布局 -->
    <style name="activity_headlayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/unify_title_height</item>
        <item name="android:background">#222222</item>
        <item name="android:padding">@dimen/layout_padding_nor</item>
    </style>
    <!-- activity或者fragment页面顶部中间侧布局 父布局相对布局 -->
    <style name="activity_headlayout_centertext">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/_18dp</item>
    </style>
    <!-- activity或者fragment页面顶部左侧布局  父布局相对布局 -->
    <style name="activity_headlayout_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/_15dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>

    <!-- activity或者fragment页面顶部中间侧布局 父布局相对布局 -->
    <style name="activity_headlayout_left_centertext">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/_18dp</item>
    </style>
    <!-- activity或者fragment页面顶部中间侧布局 父布局相对布局 -->
    <style name="activity_headlayout_center_centertext">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">@dimen/_17dp</item>
    </style>
    <!-- activity或者fragment页面顶部右侧布局 父布局相对布局 -->
    <style name="activity_headlayout_right">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">#CCffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/_14dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>
    <style name="activity_headlayout_imageView_right">
        <item name="android:layout_width">40dip</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>
    <style name="activity_headlayout_imageView_left">
        <item name="android:layout_width">40dip</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>
    <!-- 用户输入框样式 -->
    <style name="input_style">
        <item name="android:padding">@dimen/_12dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">55dp</item>
        <item name="android:textColorHint">#bbc0c7</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:background">@android:color/white</item>
    </style>
    <!-- 登录用户输入框样式 -->
    <style name="input_style_for_login">
        <item name="android:padding">@dimen/_12dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">55dp</item>
        <item name="android:textColorHint">#aaaaaa</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:background">@android:color/white</item>
    </style>
    <!-- 注册用户输入框样式 -->
    <style name="input_style_for_register">
        <item name="android:padding">@dimen/_12dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">55dp</item>
        <item name="android:textColorHint">#bbc0c7</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:background">@android:color/white</item>
    </style>

    <style name="progress_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- Dialog的windowFrame框为无 -->
        <item name="android:windowIsFloating">false</item>
        <!-- 是否漂现在activity上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 是否半透明 -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <!-- <item name="android:windowBackground">@drawable/loading_bg</item> -->
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <!-- 更改头像Dialog -->

    <style name="transparentFrameWindowStyle" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/photo_choose_bg</item>
    </style>

    <!-- 更改头像Dialog -->

    <style name="main_menu_animstyle">
        <item name="android:windowEnterAnimation">@anim/photo_dialog_in_anim</item>
        <item name="android:windowExitAnimation">@anim/photo_dialog_out_anim</item>
    </style>
    <!-- line_height_1 -->
    <style name="line_height_1">
        <item name="android:layout_height">1dip</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/line_bg_color</item>
    </style>
    <style name="line_height_10">
        <item name="android:layout_height">10dip</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/line_bg_color</item>
    </style>

    <style name="style_btn_blue_fill">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">55dip</item>
        <item name="android:padding">@dimen/view_space</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/_16sp</item>
    <item name="android:gravity">center</item>
        <item name="android:background">@drawable/btn_blue_wane_bg</item>
    </style>

    <style name="style_btn_white_fill">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">@dimen/view_space</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textColor">@color/tv_color_1</item>
        <item name="android:textSize">@dimen/_16sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/btn_white_wane_bg</item>
    </style>

    <style name="style_btn_blue_shape_fill">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:padding">@dimen/view_space</item>
    <item name="android:layout_gravity">center</item>
    <item name="android:textColor">@color/white</item>
    <item name="android:textSize">@dimen/_16sp</item>
    <item name="android:gravity">center</item>
    <item name="android:background">@drawable/btn_blue_shape_bg</item>
    </style>

    <style name="style_btn_white_shape_fill">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">@dimen/view_space</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textColor">@color/tv_color_1</item>
        <item name="android:textSize">@dimen/_16sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/btn_white_shape_bg</item>
    </style>

    <!-- 底部动画-->
    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>
    <!-- 对话框-->
    <style name="DialogStyleBottom" parent="android:Theme.Dialog">
        <item name="android:windowAnimationStyle">@style/AnimBottom</item>
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">false</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 模糊 -->
    </style>
    <!--<====================================================-->
    <style name="btn_match_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginLeft">@dimen/_10dp</item>
        <item name="android:layout_marginRight">@dimen/_10dp</item>
        <item name="android:layout_height">@dimen/btn_style_height</item>
        <item name="android:layout_marginTop">@dimen/_30dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/btn_blue_wane_bg</item>
        <item name="android:paddingBottom">@dimen/_8dp</item>
        <item name="android:paddingTop">@dimen/_8dp</item>
        <item name="android:textColor">@android:color/white</item>




    </style>
</resources>