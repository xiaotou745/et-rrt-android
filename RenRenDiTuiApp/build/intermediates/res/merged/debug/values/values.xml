    <style name="activity_headlayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:background">#222222</item>
    </style>
    <style name="activity_headlayout_center_centertext">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">@dimen/_17dp</item>
    </style>
    <style name="activity_headlayout_centertext">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/_18dp</item>
    </style>
    <style name="activity_headlayout_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/_15dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>
