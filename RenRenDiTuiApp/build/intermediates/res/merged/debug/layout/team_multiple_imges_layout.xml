<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:orientation="vertical"
    >

    <TextView
        android:id="@+id/gv_multipe_team_images_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="多组图片组"
        android:gravity="left"
        android:textColor="@color/tv_order_color_1"
        android:textSize="@dimen/_14sp"
        />
    <GridView
        android:id="@+id/gv_multipe_team_images_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnWidth="@dimen/_70dp"
        android:numColumns="5"
        android:stretchMode="columnWidth"
        android:horizontalSpacing="@dimen/view_space_half"
        android:verticalSpacing="@dimen/view_space_half"
        ></GridView>


</LinearLayout>
